{
	"author": {
		"name": "Baptiste Pillot",
		"email": "baptiste@pillot.fr"
	},
	"description": "Switch the formatting of names stored in strings between different naming conventions",
	"devDependencies": {
		"typescript": "~5.6"
	},
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"import": "./esm/rename.js",
		"require": "./cjs/rename.js"
	},
	"files": [
		"LICENSE",
		"README.md",
		"*/*.d.ts",
		"*/*.js"
	],
	"keywords": [
		"case",
		"class",
		"column",
		"convention",
		"display",
		"it.rocks",
		"live",
		"lowercase",
		"name",
		"naming",
		"rename",
		"route",
		"transform",
		"uppercase",
		"variable"
	],
	"license": "ISC",
	"name": "@itrocks/rename",
	"repository": "https://github.com/itrocks-ts/rename",
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"types": "./esm/rename.d.ts",
	"version": "0.0.8"
}
